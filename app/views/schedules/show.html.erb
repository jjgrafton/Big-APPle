<div class="schedule-page">
    <div class="container-fluid">
        <div class="row">

            <div class="schedule-layout-morning col-sm-6">
                <div class="schedule-header-morning" class="card" class="card-corner" class="card-corner-triangle">Morning</div>
                <div class="destination-box">
                    <h3>
                        <%=@museum1.name%>
                    </h3>
                </div>
                <div class="dining-box">
                    <h3>
                        <%=@restaurant1.name%>
                    </h3>
                </div>
                <%= render 'transportation' %>
            </div>


            <div class="schedule-layout-afternoon col-sm-6">
                <div class="schedule-header-afternoon">Afternoon</div>
                <div class="destination-box">
                    <h3>
                        <%=@museum2.name%>
                    </h3>
                    <p>Uber Estimate: <%= @estimate %></p>
                </div>
                <div class="dining-box">
                    <h3>
                        <%=@restaurant2.name%>
                    </h3>
                </div>
                <%= render 'transportation' %>
                </div>
            </div>
        
        </div>

    </div>
</div>